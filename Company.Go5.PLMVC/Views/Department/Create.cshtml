
@model DepartmentDto
@{
    ViewData["Title"] = "Create";
}

<h1>Create Department</h1>



<br/>
<br/>


<form asp-action="Create" >


    <div class="my-3">

        <label asp-for="Code" class="col-form-label" ></label>
        <input asp-for="Code" class="form-control"/>
        <span asp-validation-for="Code"></span>

    </div>



    <div class="my-3">

        <label asp-for="Name" class="col-form-label"></label>
        <input asp-for="Name" class="form-control" />
        <span asp-validation-for="Name"></span>

    </div>



    <div class="my-3">

        <label asp-for="CreateAt" class="col-form-label"></label>
        <input asp-for="CreateAt" class="form-control" />
        <span asp-validation-for="CreateAt"></span>

    </div>



    <div class="my-3">

        <input type="submit"  value="Create and Save" class="btn btn-primary" />
        <a asp-action="Index" asp-controller="Department" class="btn btn-light">Back to List</a>
    </div>
</form>